<PostDate>{timeConverter(post.createdAt)}</PostDate>\n    </PostContainer>\n  );\n}\n\nPost.propTypes = {\n  post: Proptypes.object,\n};\n\nexport default function HomePage() {\n  const posts = useSelector((store) => store.posts.posts);\n  const totoalPageCount = useSelector((store) => store.posts.totoalPostCount);\n  const currentPage = useSelector((store) => store.posts.currentPage);\n  const postsPerPage = 5;\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n    dispatch(getPostsCount());\n    dispatch(pagingPosts(currentPage, postsPerPage));\n  }, [dispatch, currentPage, postsPerPage]);\n\n  const handlePageClick = (page) => {\n    dispatch(setCurrentPage(page));\n  };\n\n  return (\n    <HomePageContainer>\n      {posts.map((post) => (\n        <Post key={post.id} post={post} />\n      ))}\n      <Pagination\n        currentPage={currentPage}\n        postsPerPage={postsPerPage}\n        totoalPageCount={totoalPageCount}\n        handlePageClick={handlePageClick}\n      />\n    </HomePageContainer>\n  );\n}\n","C:\\Users\\ruish\\Desktop\\Lidemy\\week 24\\blogenhanced\\src\\components\\header\\Header.js",[],"C:\\Users\\ruish\\Desktop\\Lidemy\\week 24\\blogenhanced\\src\\components\\App\\App.js",[],"C:\\Users\\ruish\\Desktop\\Lidemy\\week 24\\blogenhanced\\src\\redux\\reducers\\postsReducer.js",[],"C:\\Users\\ruish\\Desktop\\Lidemy\\week 24\\blogenhanced\\src\\serviceWorker.js",[],["125","126"],"C:\\Users\\ruish\\Desktop\\Lidemy\\week 24\\blogenhanced\\src\\utils.js",[],"C:\\Users\\ruish\\Desktop\\Lidemy\\week 24\\blogenhanced\\src\\redux\\reducers\\usersReducer.js",[],"C:\\Users\\ruish\\Desktop\\Lidemy\\week 24\\blogenhanced\\src\\pages\\SinglePostPage\\SinglePostPage.js",[],"C:\\Users\\ruish\\Desktop\\Lidemy\\week 24\\blogenhanced\\src\\pages\\LoginPage\\LoginPage.js",[],["127","128"],"C:\\Users\\ruish\\Desktop\\Lidemy\\week 24\\blogenhanced\\src\\components\\header\\index.js",[],"C:\\Users\\ruish\\Desktop\\Lidemy\\week 24\\blogenhanced\\src\\pages\\Re